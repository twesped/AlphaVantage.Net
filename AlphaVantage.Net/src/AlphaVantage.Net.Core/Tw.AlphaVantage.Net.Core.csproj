<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Tw.AlphaVantage.Net.Core</PackageId>
    <Description>Low-level wrapper for Alpha Vantage API based on HttpClient and Newtonsoft Json.Net</Description>
    <Version>1.2.0</Version>
    <Authors>LutsenkoKirill / Tom Pedersen</Authors>
    <RepositoryUrl>https://github.com/twesped/AlphaVantage.Net</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>TW.AlphaVantage.Net.Core</Product>
    <AssemblyName>Tw.AlphaVantage.Net.Core</AssemblyName>
    <RootNamespace>Tw.AlphaVantage.Net.Core</RootNamespace>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>1.2.0.0</AssemblyVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2020.1.0" />
    <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="2.2.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
</Project>